<% content_for :title, "Edit Todo List" %>

<div class="container">
  <h1 class="mb-4">Edit Todo List</h1>

  <%= render "form", todo_list: @todo_list %>

  <%= link_to todo_list_path(@todo_list), class: "btn btn-secondary mt-4" do %>
